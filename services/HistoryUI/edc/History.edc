collections {
    group{
        name: "elm/button/base/history_button";
        images {
            image: "btn_bar_clear_dis.png" COMP;
            image: "btn_bar_clear_foc.png" COMP;
            image: "btn_bar_clear_nor.png" COMP;
        }
        parts{
            part {
                name: "clearhistory_bg";
                scale: 1;
                mouse_events: 1;
                repeat_events: 0;
                type: RECT;
                description {
                    state: "default" 0.0;
                    visible: 1;
                    color: 0 0 0 0;
                }
                description {
                    state: "disable" 0.0;
                    inherit: "default" 0.0;
                }
                description {
                    state: "focus" 0.0;
                    inherit: "default" 0.0;
                    color: 0 119 246 255;
                }
            }
            part {
                name: "clearhistory_ic";
                scale: 1;
                mouse_events: 1;
                repeat_events: 1;
                type: IMAGE;
                description {
                    state: "default" 0.0;
                    visible: 1;
                    align: 0.5 0.5;
                    fixed: 1 1;
                    image.normal: "btn_bar_clear_nor.png";
                    rel1 {
                        relative: 0.0 0.0;
                        to: "clearhistory_bg";
                    }
                    rel2 {
                        relative: 1.0 1.0;
                        to: "clearhistory_bg";
                    }
                }
                description {
                    state: "focus" 0.0;
                    inherit: "default" 0.0;
                    image.normal: "btn_bar_clear_foc.png";
                }
                description {
                    state: "disable" 0.0;
                    inherit: "default" 0.0;
                    image.normal: "btn_bar_clear_dis.png";
                }
            }
        }
        programs{
            program {
                name: "mouse_click";
                signal: "mouse,clicked,1";
                source: "clearhistory_bg";
                script {
                    emit("elm,action,click", "");
                }
            }
            program {
                name: "mouse_in_nor_history";
                signal: "mouse,in";
                source: "clearhistory_bg";
                action: STATE_SET "focus" 0.0;
                target: "clearhistory_ic";
                target: "clearhistory_bg";
            }
            program {
                name: "mouse_out_nor_history";
                signal: "mouse,out";
                source: "clearhistory_bg";
                action: STATE_SET "default" 0.0;
                target: "clearhistory_ic";
                target: "clearhistory_bg";
            }
        }
    }
    group{
        name: "elm/button/base/clear_history_button";
        images {
            image: "btn_bar_clear_all_dis.png" COMP;
            image: "btn_bar_clear_all_foc.png" COMP;
            image: "btn_bar_clear_all_nor.png" COMP;
        }
        parts{
            part {
                name: "clearhistory_bg";

                description {
                    state: "default" 0.0;
                    visible: 1;
                    color: 0 0 0 0;
                }
                description {
                    state: "disable" 0.0;
                    inherit: "default" 0.0;
                }
                description {
                    state: "focus" 0.0;
                    inherit: "default" 0.0;
                    color: 0 119 246 255;
                }
            }
            part {
                name: "clearhistory_ic";
                scale: 1;
                mouse_events: 1;
                repeat_events: 1;
                type: IMAGE;
                description {
                    state: "default" 0.0;
                    visible: 1;
                    align: 0.5 0.5;
                    fixed: 1 1;
                    image.normal: "btn_bar_clear_all_nor.png";
                    rel1 {
                        relative: 0.0 0.0;

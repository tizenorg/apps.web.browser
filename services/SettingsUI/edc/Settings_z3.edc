#define __CONCAT(X,Y)          X##Y

#define DEBUG_RECT_OVER(over_part, r, g, b)                         \
    part { name: __CONCAT("dbg_rect_at_", __stringify(__LINE__));   \
                                                                    \
scale:1;                                                            \
        type : RECT;                                                \
        repeat_events: 1;                                           \
        description {                                               \
            state: "default" 0.0;                                   \
            visible: 1;                                             \
            color: r g b 128;                                       \
            rel1 { to: over_part; relative: 0 0; }                  \
            rel2 { to: over_part; relative: 1 1; }                  \
        }                                                           \
    }

#define SETTINGS_LINE(line_name, relative_to)                       \
    part {                                                          \
        name: line_name;                                            \
        scale:1;                                                    \
        type: RECT;                                                 \
        description {                                               \
            state: "default" 0.0;                                   \
            visible: 1;                                             \
            fixed: 1 1;                                             \
            min: 360 2;                                             \
            max: 360 2;                                             \
            align: 0 0;                                             \
            rel1 { relative: 0.0 1.0; to: relative_to;}             \
            rel2 { relative: 1.0 1.0;}                              \
            color: 0 0 0 13;                                        \
        }                                                           \
    }

#define SETTINGS_ITEM_PROGRAM(prog_name)                            \
    program {                                                       \
        name: __CONCAT("mouse_click", prog_name);                   \
        signal: "mouse,clicked,1";                                  \
        source: __CONCAT(prog_name, "_over");                       \
        script {                                                    \
            emit("elm,action,click", "");                           \
        }                                                           \
    }                                                               \
    program {                                                       \
        name: __CONCAT("mouse_down_", prog_name);                   \
        signal: "mouse,down,*";                                     \
        source: __CONCAT(prog_name, "_*");                          \
        action: STATE_SET "highlight" 0.0;                          \
        target: __CONCAT(prog_name, "_button");                     \
    }                                                               \
    program {                                                       \
        name: __CONCAT("mouse_up_", prog_name);                     \
        signal: "mouse,up,*";                                       \
        source: __CONCAT(prog_name, "_*");                          \
        action: STATE_SET "default" 0.0;                            \
        target: __CONCAT(prog_name, "_button");                     \
    }

#define TEXT_WITH_CHBOX(twc_name, rel_to_part, twc_text)                        \
    part{                                                                       \
        name: __CONCAT(twc_name, "_spacer");                                    \

collections {

#define INDICATOR 52
#define LAYOUT_WIDTH 380 //720
#define LAYOUT_HEIGHT 500+INDICATOR   //1120+INDICATOR
#define LAYOUT_TITLE_HEIGHT 60//80//108

#define SMALL_TILE_WIDTH 160//319
#define SMALL_TILE_HEIGHT 180 //361

#define MORE_SWALLOW_WIDTH 480
#define MORE_SWALLOW_HEIGHT 360
#define MORE_WIDTH 467
#define MORE_HEIGHT 360
#define MORE_ITEM_HEIGHT 120
#define TEXT_WIDTH 180 //401
#define TEXT_HEIGHT 20 //38
#define TEXT_OFFSET_X 33
#define TEXT_OFFSET_Y 41

    group {
        name: "topContent";
        min: LAYOUT_WIDTH LAYOUT_HEIGHT;
        max: LAYOUT_WIDTH LAYOUT_HEIGHT;
        images {
            image: "navi_ic_stop.png" COMP;
        }
        data.item: "texts" "webpage_title";
        data.item: "contents" "favicon close_click";
        color_classes{
            color_class{
                name: "focusBgColor";
                color: 224 237 251 255;
            }
            color_class{
                name: "focusDelBgColor";
                color: 96 114 146 255;
            }
            color_class{
                name: "focusTextColor";
                color: 255 255 255 255;
            }
            color_class{
                name: "transparent";
                color: 0 0 0 0;
            }
        }
        parts {
            part {
                name: "bg_rect";
                scale:1;
                mouse_events: 1;
                type: RECT;
                description {
                    state: "default" 0.0;
                    color: 231 231 231 255;
                    align: 0 0;
                    min: LAYOUT_WIDTH LAYOUT_TITLE_HEIGHT;
                    max: LAYOUT_WIDTH LAYOUT_TITLE_HEIGHT;
                    rel1 { relative: 0.0 0.0; offset: 0 0; }
                }
            }
            part {
                name: "action_bar_bg";
                type: RECT;
                scale: 1;
                description { state: "default" 0.0;
                    visible: 1;
                    fixed: 1 1;
                    min: LAYOUT_WIDTH LAYOUT_TITLE_HEIGHT;
                    max: LAYOUT_WIDTH LAYOUT_TITLE_HEIGHT;
                    align: 0 0;
                    rel1 { relative: 0.0 0.0;to: "bg_rect"; offset: 0 0;}
                    rel2 { relative: 1.0 1.0;to: "bg_rect";}
                    color : 255 255 255 255;
                }
            }
            part {
                name: "title_text";
                scale: 1;
                type: TEXT;
                description { state: "default" 0.0;
                  visible: 1;
                  fixed: 1 1;
                  rel1 { relative: 0.0 0.0;to: "action_bar_bg";}
                  rel2 { relative: 1.0 1.0;to: "action_bar_bg";}
                  color: 51 51 51 255;
                  text {
                      text: "Bookmark Manager";
                      font: "Sans";
                      size: 18; //42
                      align: 0.5 0.5;
                     }
                  }
            }
            part {
              name: "close_bg";
              scale:1;
              mouse_events: 1;
              type: RECT;
              description {
                  state: "default" 0.0;
                  visible: 1;
                  fixed: 1 1;
                  align: 0 0.5;
                  min: 40 40;
                  max: 40 40;
                  rel1 { relative: 0.0 0.0; to: "action_bar_bg"; offset: 32 0;}
                  rel2 { relative: 1.0 1.0; to: "action_bar_bg";}
                  color_class: transparent;
              }
              description {
                  state: "highlight" 0.0;
                  inherit: "default" 0.0;
                  color_class: focusBgColor;
                  visible: 1;
              }
                description {
                  state: "focus" 0.0;
                  inherit: "default" 0.0;
                  color_class: focusDelBgColor;
                  visible: 1;
              }
            }
            part {
              name: "close_icon";
              type: IMAGE;
              scale: 1;
              description { state: "default" 0.0;
                  visible: 1;
                  fixed: 1 1;
                  align: 0 0;
                  min: 40 40;//64
                  max: 40 40;
                  rel1 { relative: 0.0 0.0; to: "close_bg";}
                  rel2 { relative: 1.0 1.0; to: "close_bg";}
                  image.normal: "navi_ic_stop.png";
              }
              description { state: "highlight" 0.0;
                  inherit: "default" 0.0;
                  visible: 1;
              }
              description { state: "focus" 0.0;
                  inherit: "highlight" 0.0;
              }
            }
            part {
              name: "close_over";
              scale:1;
              mouse_events: 1;
              type: RECT;
              description {
                  state: "default" 0.0;
                  visible: 1;
                  align: 0 0;
                  fixed: 1 1;
                  min: 40 40;
                  max: 40 40;
                  rel1 { relative: 0.0 0.0; to: "close_bg";}
                  rel2 { relative: 1.0 1.0; to: "close_bg";}
                  color_class: transparent;
              }
            }
            part {
              name: "close_click";
              scale:1;
              mouse_events: 1;
              type: SWALLOW;
              description {
                  state: "default" 0.0;
                  visible: 1;
                  align: 0 0;
                  fixed: 1 1;
                  min: 40 40;
                  max: 40 40;
                  rel1 { relative: 0.0 0.0; to: "close_over";}
                  rel2 { relative: 1.0 1.0; to: "close_over";}
                  color_class: transparent;
              }
            }
        }
        programs{
          program {
              name: "mouse_in_close_click";
              signal: "mouse,in";
              source: "close_click";
              script {
                  emit("mouse_in_close_click", "");
              }
          }
          program {
              name: "mouse_out_close_click";
              signal: "mouse,out";
              source: "close_click";
              script {
                  emit("mouse_out_close_click", "");
              }
          }
          program {
              name: "mouse_in_close";
              signal: "mouse_in_close_click";
              source: "";
              action: STATE_SET "highlight" 0.0;
              target: "close_over";
              target: "close_bg";
              target: "close_icon";
          }
          program {
              name: "mouse_out_close";
              signal: "mouse_out_close_click";
              source: "";
              action: STATE_SET "default" 0.0;
              target: "close_over";
              target: "close_bg";
              target: "close_icon";
          }
      }
    }

    group {
        name: "elm/button/base/more-button";
        styles {
            style {
                name: "more_style";
                base: "font=Tizen:style=Regular font_size=30 wrap=none color=#333333 align=0.0 valign=0.5"; //38
            }
        }
        parts {
            part {
                name: "shadow_left";
                type: RECT;
                scale: 1;
                description {
                    state: "default" 0.0;
                    align: 0 0;
                    visible: 1;
                    min: 3 MORE_ITEM_HEIGHT;
                    max: 3 MORE_ITEM_HEIGHT;
                    color: 0 0 0 51;
                    rel1.relative: 0.0 0.0;
                    rel2.relative: 1.0 1.0;
                }
            }
            part {
                name: "shadow_bot";
                type: RECT;
                scale: 1;
                description {
                    state: "default" 0.0;
                    align: 0 1;
                    visible: 1;
                    min: MORE_WIDTH-6 3;
                    max: MORE_WIDTH-6 3;
                    color: 0 0 0 51;
                    rel1 {
                        relative: 1.0 0.0;
                        to: "shadow_left";
                    }
                    rel2.relative: 1.0 1.0;
                }
                description {
                    state: "visible" 0.0;
                    inherit: "default";
                    visible: 0;
                }
            }
            part {
                name: "bg";
                scale: 1;
                type: RECT;
                effect: FAR_SHADOW BOTTOM_LEFT;
                description {
                    state: "default" 0.0;
                    align: 1 0;
                    min: MORE_WIDTH-3 MORE_ITEM_HEIGHT-3;
                    max: MORE_WIDTH-3 MORE_ITEM_HEIGHT-3;
                    rel1.relative: 0.0 0.0;
                    rel2.relative: 1.0 1.0;
                    color: 255 255 255 255;
                    visible: 1;
                }
                description {
                    state: "pressed" 0.0;
                    inherit: "default";
                    color: 224 237 251 255;
                }
            }

            part {
                name: "text_spacer";
                type: RECT;
                scale: 1;
                description {
                    state: "default";
                    align: 0.0 0.0;
                    min: TEXT_OFFSET_X TEXT_OFFSET_Y;
                    max: TEXT_OFFSET_X TEXT_OFFSET_Y;
                    visible: 0;
                    rel1 {
                        relative: 0.0 0.0;
                        to: "bg";
                    }
                    rel2.relative: 1.0 1.0;
                }
            }
            part {
                name: "elm.text";
                scale: 1;
                type: TEXTBLOCK;
                description {
                    state: "default" 0.0;
                    align: 0.0 0.0;
                    min: TEXT_WIDTH TEXT_HEIGHT;
                    max: TEXT_WIDTH TEXT_HEIGHT;
                    visible: 1;
                    rel1 {
                        relative: 1.0 1.0;
                        to: "text_spacer";
                    }
                    rel2.relative: 1.0 1.0;
                    text.style: "more_style";
                }
            }
            part {
                name: "divider";
                scale: 1;
                type: RECT;
                description {
                    state: "default";
                    align: 0.0 1.0;
                    visible: 0;
                    rel1.relative: 0.0 0.0;
                    rel2.relative: 1.0 1.0;
                    min: MORE_WIDTH 1;
                    max: MORE_WIDTH 1;
                    color: 0 0 0 26;
                }
                description {
                    state: "visible" 0.0;
                    inherit: "default";
                    visible: 1;
                }
            }
            part {
                name: "over";
                scale: 1;
                mouse_events: 1;
                repeat_events: 0;
                type: RECT;
                description {
                    state: "default" 0.0;
                    visible: 1;
                    rel1 {
                        relative: 0.0 0.0;
                        to: "bg";
                    }
                    rel2 {
                        relative: 1.0 1.0;
                        to: "bg";
                    }
                    color: 0 0 0 0;
                }
            }
        }
        programs {
            program {
                name: "visible";
                source: "ui";
                signal: "visible";
                action: STATE_SET "visible" 0.0;
                target: "divider";
                target: "shadow_bot";
            }
            program {
                name: "down";
                source: "over";
                signal: "mouse,down,*";
                action: STATE_SET "pressed" 0.0;
                target: "bg";
            }
            program {
                name: "up";
                source: "over";
                signal: "mouse,up,*";
                action: STATE_SET "default" 0.0;
                target: "bg";
            }
            program {
                name: "mouse_click";
                signal: "mouse,clicked,1";
                source: "over";
                script {
                    emit("elm,action,click", "");
                }
            }
        }
    }


    group {
        name: "bookmarkmanager-layout";
        data {
            item: "highlight_focus" "off";
        }

        parts {
            part { name: "window_bg";
                type: RECT;
                scale: 1;
                mouse_events: 0;
                description { state: "default" 0.0;
                              align: 0.0 0.0;
                              visible: 1;
                              min: LAYOUT_WIDTH LAYOUT_HEIGHT;
                              max: LAYOUT_WIDTH LAYOUT_HEIGHT;
                              color: 231 231 231 255;
                              rel1 {
                                  relative: 0 0;
                              }
                              rel2{
                                  relative: 1 1;

        }

        part { name: "elm.thumbnail";
            type: SWALLOW;
            scale: 1;
            description { state: "default" 0.0;
                fixed: 1 0;
                align: 0.0 0.0;
                color : 75 102 163 200;
                min: SMALL_TILE_WIDTH (SMALL_TILE_HEIGHT-100);
                max: SMALL_TILE_WIDTH (SMALL_TILE_HEIGHT-100);
                rel1 {
                    relative: 0.0  0.0; to: "bg";
                }
                rel2 {
                    relative: 1.0  1.0; to: "bg";
                }
            }
            description { state: "selected";
                inherit: "default" 0.0;
            }
        }

        part {
              name: "icon";
              type: IMAGE;
              scale: 1;
              description { state: "default" 0.0;
                  visible: 1;
                  fixed: 1 1;
                  align: 0.5 0.5;
                  min: 99 99;
                  max: 99 99;
                  rel1 { relative: 0.0 0.0; to: "elm.thumbnail";}
                  rel2 { relative: 1.0 1.0; to: "elm.thumbnail";}
                  image.normal: "folder_ic_bookmark.png";
              }
          }

        part { name: "background";
            type: RECT;
            scale: 1;
            description { state: "default" 0.0;
                min: SMALL_TILE_WIDTH 100;
                max: SMALL_TILE_WIDTH 100;
                align: 0.0 0.0;
                color: 75 102 163 255;
                rel1 {
                    to: "elm.thumbnail";
                    relative: 0.0  1.0;
                }
                rel2 {
                    relative: 1.0  1.0;
                }
            }
            description { state: "selected" 0.0;
                inherit: "default" 0.0;
                color: 75 102 163 200;
            }
        }

        part { name: "page_title";
            type: TEXT;
            scale: 1;
            description { state: "default" 0.0;
                min: 300 48;
                max: 300 48;
                align: 0.0 0.5;
                color: 255 255 255 255;
                rel1 {
                    to: "background";
                    relative: 0.0 0.0;
                    offset: 36 0;
                }
                rel2 {
                    to: "background";
                    relative: 1.0  1.0;
                }
                text {
                    text: "Mobile";
                    font: "Sans";
                    size: 27;
                    align: 0 0.5;
                }
            }
        }

        part { name: "elm.thumbButton";
            type: SWALLOW;
            scale: 1;
            description { state: "default" 0.0;
                rel1.to: "elm.thumbnail";
                rel2.to: "elm.thumbnail";
            }
        }

        part { name: "over";
            type: RECT;
            scale: 1;
            mouse_events: 1;
            repeat_events: 1;
            description { state: "default" 0.0;
                color: 0 0 0 0;
                rel1.to: "bg";
                rel2.to: "background";
            }
        }
    }

    programs{
        program { name: "mouse_in";
            signal: "mouse,in";
            source: "over";
            action:  STATE_SET "selected" 0.0;
            target:  "background";
            target: "bg";
        }
        program { name: "mouse_out";
            signal: "mouse,out";
            source: "over";
            action:  STATE_SET "default" 0.0;
            target:  "background";
            target: "bg";
        }
    }
}

}

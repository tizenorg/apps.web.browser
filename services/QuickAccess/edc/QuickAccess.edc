			}
		}
	}

	programs{
		program { name: "mouse_in";
			signal: "mouse,in";
			source: "over";
			action:  STATE_SET "selected" 0.0;
			target:  "background";
			target: "focus_highlight";
			target: "bg";
		}
		program { name: "mouse_out";
			signal: "mouse,out";
			source: "over";
			action:  STATE_SET "default" 0.0;
			target:  "background";
			target: "focus_highlight";
			target: "bg";
		}
	}
}

